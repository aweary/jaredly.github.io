        },
        {
          "_id": null,
          "_rev": null,
          "children": [],
          "completed": null,
          "content": "~$ tree\n.\n├── bsconfig.json\n├── package.json\n├── webpack.config.js\n├── public\n│   ├── index.html\n│   └── styles.css\n└── src\n    ├── Main.re\n    └── TodoList.re",
          "created": 1498052577612,
          "modified": 1498951288387,
          "parent": null,
          "plugins": {},
          "trashed": null,
          "type": "code",
